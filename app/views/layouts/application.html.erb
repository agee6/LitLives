<!DOCTYPE html>
<html>
<head>
  <title>LitLives</title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
</head>
<body>
  <div class="logo" onclick="location.href='/'">
  </div>

    <% if logged_in? %>
    <h4 class="header" id="username"><%= current_user.username %></h4>
    <%= button_to "SignOut", session_url, method: :delete, class: "headerButton" %>
    <% end %>


<%= yield %>


</body>
</html>
