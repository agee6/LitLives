<!DOCTYPE html>
<html>
<head>
  <title>LitLives</title>
  <%= stylesheet_link_tag    'application', media: 'all' %>
  <%= javascript_include_tag 'application' %>
  <%= csrf_meta_tags %>
  <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/font-awesome/4.5.0/css/font-awesome.min.css">

</head>
<body>
  <div class="logo" onclick="location.href='/'">
  </div>

    <% if logged_in? %>
    <h4 class="header" id="username"><%= current_user.username %></h4>
    <%= button_to "SignOut", session_url, method: :delete, class: "headerButton" %>
    <% end %>
  <% if flash[:errors] %>
    <p><%= flash[:errors] %></p>
  <% end %>



<%= yield %>


</body>
</html>
